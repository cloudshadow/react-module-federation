{
  "name": "store-scheduling-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start-test": "node ./src/bin/www",
    "start": "nodemon --exec \"npm run server\" --ignore public/js",
    "server": "babel-node ./src/bin/www.js"
  },
  "dependencies": {
    "apollo-datasource-rest": "^0.14.0",
    "apollo-server-express": "^2.9.15",
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "decimal.js": "^10.1.1",
    "express": "^4.16.4",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.4",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.0",
    "sequelize": "^4.43.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "6.5.4",
    "babel-eslint": "^8.2.6",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "mongodb": "^3.1.13",
    "mongoose": "^5.4.19",
    "mssql": "^5.0.2",
    "nodemon": "^1.18.10",
    "sequelize": "^4.43.0",
    "sequelize-auto": "^0.4.29",
    "tedious": "^5.0.3"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "rules": {
      "brace-style": [
        "error"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-unused-vars": [
        "warn"
      ],
      "no-var": [
        "off"
      ],
      "no-console": [
        "off"
      ],
      "one-var": [
        "off"
      ]
    }
  }
}
